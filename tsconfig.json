{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "es2020",
        "moduleResolution": "node",
        "module": "esnext",
        "noEmit": true,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "types": []
    },
    "include": ["src/**/*"]
}